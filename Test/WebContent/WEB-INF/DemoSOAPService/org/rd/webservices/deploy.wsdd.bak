<!-- Use this file to deploy some handlers/chains and services      -->
<!-- Two ways to do this:                                           -->
<!--   java org.apache.axis.client.AdminClient deploy.wsdd          -->
<!--      after the axis server is running                          -->
<!-- or                                                             -->
<!--   java org.apache.axis.utils.Admin client|server deploy.wsdd   -->
<!--      from the same directory that the Axis engine runs         -->

<deployment
    xmlns="http://xml.apache.org/axis/wsdd/"
    xmlns:java="http://xml.apache.org/axis/wsdd/providers/java">

  <!-- Services from DemoSOAPService WSDL service -->

  <service name="DemoSOAP" provider="java:RPC" style="wrapped" use="literal">
      <parameter name="wsdlTargetNamespace" value="http://webservices.rd.org"/>
      <parameter name="wsdlServiceElement" value="DemoSOAPService"/>
      <parameter name="schemaQualified" value="http://webservices.rd.org"/>
      <parameter name="wsdlServicePort" value="DemoSOAP"/>
      <parameter name="className" value="org.rd.webservices.DemoSOAPSoapBindingImpl"/>
      <parameter name="wsdlPortType" value="DemoSOAP"/>
      <parameter name="typeMappingVersion" value="1.2"/>
      <operation name="sayHelloWorld" qname="operNS:sayHelloWorld" xmlns:operNS="http://webservices.rd.org" returnQName="retNS:sayHelloWorldReturn" xmlns:retNS="http://webservices.rd.org" returnType="rtns:string" xmlns:rtns="http://www.w3.org/2001/XMLSchema" soapAction="" >
        <parameter qname="pns:name" xmlns:pns="http://webservices.rd.org" type="tns:string" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
      </operation>
      <parameter name="allowedMethods" value="sayHelloWorld"/>

  </service>
</deployment>
